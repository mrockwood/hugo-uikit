{
  "name": "tris",
  "version": "1.0.0",
  "description": "A webpack boilerplate for static websites that has all the necessary modern tools and optimizations built-in.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --minify",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrockwood/tris"
  },
  "keywords": [],
  "author": "mrockwood",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.14.0",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "hugo-bin": "^0.37.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "lazysizes": "^4.1.5"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
